        <script type="text/javascript">
        jQuery(document).ready(function($) {
            // Global Newsletter - Checks newsletter opt-in checkbox.
            var initialCheckboxStage = true;

            $('.bronto-check').change(function() {
                // Avoid checking opt-in checkbox state on inital page load.
                // Only check afterwards.
                if (!initialCheckboxStage) {
                    var currentForm = $(this).closest('form');
                    if ($(this).is(':checked')) {
                        currentForm.removeClass('form-error form-check');
                        currentForm.find('.newsletter-submit').removeAttr('disabled');
                    } else {
                        currentForm.addClass('form-error form-check');
                        currentForm.find('.newsletter-submit').attr('disabled', '');
                    }
                } else {
                    initialCheckboxStage = false;
                }
            });

            // Global Newsletter - Checks input and checkbox targeting parent
            // container fo submit button.
            $('.input_submit').on('click', function() {
                var currentForm  = $(this).closest('form'),
                    currentCheck = currentForm.find('.bronto-check'),
                    currentEmail = currentForm.find('.newsletter__input').val();

                if (currentCheck.is(':checked') && currentEmail.length > 4) {
                    currentForm.removeClass('.form-error');
                    currentForm.find('.newsletter__input').removeAttr('style');
                    return true;
                } else if (!currentCheck.is(':checked')) {
                    currentForm.addClass('form-error form-check');
                    currentForm.find('.opt-in-copy').focus();
                    return false;
                } else {
                    currentForm.addClass('form-error form-input');
                    currentForm.find('.newsletter__input').focus().css('border','1px solid red');
                    return false;
                }
            });

            var chk = $('#opt-in'),
                btn = $('.newsletter-submit');

            $(chk).change(function() {
                $(btn).prop("disabled", !this.checked);
            }).change();

            $("#customer_form_id").on("submit", function(e) {
                e.preventDefault();

                $(btn).prop("disabled", true);

                var newsBaseUrl = window.location,
                    newsBaseUrlRegion = newsBaseUrl.pathname,
                    newsBaseUrlRegion = newsBaseUrlRegion.replace(/\//g, ''),
                    newsApiId  = "",
                    newsListId = "",
                    newsEmail  = "",
                    currentForm  = $(this),
                    currentEmail = currentForm.find('.newsletter__input');

                switch (newsBaseUrlRegion) {
                    case "uk":
                        newsApiId  = "buthwcbtagizyrxiwxvrhdaqqcmrbbj";
                        newsListId = "0be003ec00000000000000000000000a7673";
                        break;
                    case "eu":
                        newsApiId  = "bivjxxggptlepphrgosuirynwrdcbll";
                        newsListId = "0be003ec00000000000000000000000a7668";
                        break;
                    case "fr":
                        newsApiId  = "bkxrpejdyyuhlloeqrpvdvtshyyrbne";
                        newsListId = "0be003ec00000000000000000000000a766a";
                        break;
                    case "de":
                        newsApiId  = "aktugwjtsaocfinbizffcmhykszcbfm";
                        newsListId = "0be003ec00000000000000000000000a749d";
                        break;
                    case "au":
                        newsApiId  = "agjqipfioxqrpsnpeofiygwyhrezbeh";
                        newsListId = "0be003ec00000000000000000000000a8a10";
                        break;
                    case "nz":
                        newsApiId  = "aunwduqgvhycgpunxmgbmztfhpspbad";
                        newsListId = "0be003ec00000000000000000000000a8a11";
                        break;
                    default:
                        // US
                        newsApiId  = "bxrwutzyezfblxlkeupmucvyofiubei";
                        newsListId = "0be003ec00000000000000000000000a6a84";
                }

                var newsApiUrl = "https://app.bronto.com/public/?q=direct_add&fn=Public_DirectAddForm&id=" + newsApiId + "&email=" + currentEmail.val() + "&list1=" + newsListId + "&createCookie=1";
                currentForm.append('<img class="bronto-img" src="' + newsApiUrl + '" width="0" height="0" border="0" alt="" />');
                currentForm.find('.newsletter-submit').attr('disabled','');
                currentForm.find('.form-success-wrapper').show();
                currentForm.find('.newsletter-form, .success-hide').hide();
            });
        });
        </script>
